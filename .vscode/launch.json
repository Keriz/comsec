    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "attach",
            "program": "/data/gthivolet/main",
            "processId": "${command:pickProcess}",
            "args": [],
            "stopAtEntry": true,
            "cwd": "/data/gthivolet/",
            "MIMode": "gdb",
            "miDebuggerServerAddress": "ee-tik-cn002:2346",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}